unit __Math;

interface

uses PhpLib;

function Ceil(const A: extended): extended; external;
function Cos(const A: extended): extended; external;
function Floor(const A: extended): extended; external;
function Power(const A, B: extended): extended;
function Sin(const A: extended): extended; external;

implementation

function Power(const A, B: extended): extended;
begin
  Result := php_pow(A, B);
end;

end.

  //readln(s);
  s:='&lt;&xyz;&gt;';
  WriteLn(StdErr, strDecodeHTMLEntities('Hallo:&#x20;&#x3C;&auml;&szlig;&nbsp; &lt; &gt;',eUTF8));
  WriteLn(StdErr, strDecodeHTMLEntities(s,eUTF8));
  WriteLn(StdErr, strDecodeHTMLEntities(s,eUTF8,false));
  except
  end;         }
end.

